{
  "name": "@consenlabs-fe/eslint-config",
  "version": "0.3.5",
  "main": "index.js",
  "license": "MIT",
  "description": "Eslint configuration for Consenlabs team",
  "repository": {
    "type": "git",
    "url": "https://github.com/consenlabs/eslint"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "prettier": "@consenlabs-fe/prettier",
  "scripts": {
    "build": "echo 0",
    "sync-subpackage": "npm i @consenlabs-fe/eslint-plugin-custom --workspace=@consenlabs-fe/eslint-config-ts",
    "sync-version": "node scripts/release.js && npm run sync-subpackage",
    "release": "npm run sync-version && npm publish --workspaces"
  },
  "devDependencies": {
    "@consenlabs-fe/prettier": "^0.1.2",
    "@types/node": "^14.11.8",
    "prettier": "^2.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
